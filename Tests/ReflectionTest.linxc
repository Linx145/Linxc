#include <Reflection.h>
#include <Test.h>

int main()
{
    Reflection::initReflection();

    Reflection::Type *type = typeof(StructA);
    StructA instance = StructA(4);

    int* variable = (int*)type->GetVariable("integer", &instance);
    *variable = 5;

    printf("%d\n", instance.integer);

    return 0;
}