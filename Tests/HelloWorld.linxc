#include <Linxc.h>
#include <stdio.linxch>
#include <TestArray.linxc>

bool I32Equals(i32 A, i32 B)
{
    return A == B;
}

i32 main()
{
    collections::TestArray<i32> var;
    var.data = (i32*)malloc(4 * 4); //crashes here
    var.data[0] = 1;
    var.data[1] = 2;
    var.data[2] = 3;
    var.data[3] = 4;
    if (var.Contains(5, &I32Equals))
    {
        printf("Contains 5\n");
    }
    if (var.Contains(2, &I32Equals))
    {
        printf("Contains 2\n");
    }
    return 0;
}