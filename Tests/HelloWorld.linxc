#include <stdint.h>
#include <stdio.h>
#include <Linxc.h>

delegate(exampleDelegateType, Vector2, float, float);

struct Vector2
{
    float x;
    float *y;

    exampleDelegateType exampleDelegate;

    bool Equals(Vector2 other)
    {
        return x == other.x && y == other.y;
    }
    float Dot(Vector2 other)
    {
        return x * other.x + *y * *other.y;
    }
    Vector2 CallDelegate(float x, float y)
    {
        return exampleDelegate(x, y);
    }
};