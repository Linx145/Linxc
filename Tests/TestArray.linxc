#include <Linxc.h>
#include <stdlib.linxch>

namespace collections
{
    template<typename T>
    struct TestArray
    {
        def_delegate(EqlFunc, bool, T, T);

        T *data;
        u64 length;

        void deinit()
        {
            free(data);
        }
        bool Contains(T input, EqlFunc eqls)
        {
            for (u64 i = 0; i < length; i++)
            {
                if (eqls(data[i], input))
                {
                    return true;
                }
            }
            return false;
        }
    };
}